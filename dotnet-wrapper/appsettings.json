{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "BasePath": "/test-generator",
  "OPENAI_API_KEY": "",
  "OPENAI_MODEL": "gpt-4o-mini",
  "ReverseProxy": {
    "Routes": {
      "test-generator-route": {
        "ClusterId": "nodejs-cluster",
        "Match": {
          "Path": "/test-generator/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "root-route": {
        "ClusterId": "nodejs-cluster", 
        "Match": {
          "Path": "/test-generator"
        },
        "Transforms": [
          {
            "PathPattern": "/"
          }
        ]
      }
    },
    "Clusters": {
      "nodejs-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:3000/"
          }
        }
      }
    }
  }
}
