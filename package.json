{
  "name": "e-e-test-generator",
  "version": "1.0.0",
  "description": "Prototype full-stack AI-powered test generator",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.56.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!server.js",
      "!*.test.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
